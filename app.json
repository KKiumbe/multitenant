{
  "expo": {
    "name": "TaQa",
    "entryPoint": ".app/(tabs)/index.js",
    "slug": "client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./icon.png",  
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./icon.png",  
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
   
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./icon.png",  
        "backgroundColor": "#ffffff"
      },
      "package": "com.kiumbe.taqa"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./icon.png" 
    },
    "plugins": [
      "expo-router"
       
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b1383cc6-df12-4e8c-a9bb-31dacce4a1ef"
      }
    }
  }
}
